{
	"name": "try-turborepo-next-nest-npm",
	"private": true,
	"scripts": {
		"build": "turbo run build",
		"dev": "turbo run dev",
		"lint": "turbo run lint",
		"format": "prettier --write \"**/*.{ts,tsx,md}\"",
		"check-types": "turbo run check-types",
		"test": "turbo test"
	},
	"devDependencies": {
		"prettier": "^3.7.4",
		"turbo": "^2.7.2",
		"typescript": "5.9.2"
	},
	"engines": {
		"node": ">=18"
	},
	"packageManager": "npm@11.6.1",
	"workspaces": [
		"apps/*",
		"packages/*"
	]
}
